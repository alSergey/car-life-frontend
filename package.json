{
  "name": "car-life",
  "version": "1.0.0",
  "description": "VK Mini App CarLife",
  "main": "index.js",
  "scripts": {
    "start:local": "cross-env PORT=10888 HTTPS=false BE_URL=http://localhost:8080 react-scripts start",
    "start": "cross-env PORT=10888 HTTPS=false BE_URL=https://car-life.site react-scripts start",
    "build": "react-scripts build",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=http --ws-protocol=wss --host=localhost --port=10888",
    "generate:local": "cross-env BE_URL=http://localhost:8080 node scripts/generate.js",
    "generate": "cross-env BE_URL=https://car-life.site node scripts/generate.js",
    "eslint": "eslint --ext .js,jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "tscompile": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run tscompile",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run tscompile",
    "update:deps": "npm-check -u"
  },
  "author": "Alekhin Sergey",
  "license": "MIT",
  "dependencies": {
    "@happysanta/router": "^0.3.1",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@vkontakte/icons": "^1.185.0",
    "@vkontakte/vk-bridge": "^2.7.0",
    "@vkontakte/vk-tunnel": "^0.1.2",
    "@vkontakte/vkjs": "^0.23.0",
    "@vkontakte/vkui": "^4.35.3",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-yandex-maps": "4.6.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eruda": "^2.5.0",
    "eruda-code": "^2.0.0",
    "eruda-dom": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "npm-check": "^6.0.1",
    "prettier": "^2.7.1",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^4.0.3",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "swagger-typescript-api": "^10.0.1",
    "typescript": "^4.7.4",
    "utility-types": "^3.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
